[access]
	inheritFrom = All-Projects
[submit]
[access "refs/*"]
	owner = group user/pybok81
[access "refs/for/refs/*"]
	label-Code-Review = -2..+2 group user/pybok81
	label-Verified = -1..+1 group user/pybok81
	push = group user/pybok81
	read = group user/pybok81
	rebase = group user/pybok81
	removeReviewer = group user/pybok81
	submit = group user/pybok81
[access "refs/heads/*"]
	create = group user/pybok81
	push = +force group user/pybok81
	pushMerge = group user/pybok81
	read = group user/pybok81
[access "refs/tags/*"]
	push = group user/pybok81
	push = +force group user/pybok81
